
<% switch(status) {
  case 'unvisited' : %>
  <div class="centered-container <%= isKidLocation ? 'kid-location' : '' %>">
    <div class="svg-wrapper">
      <form action="/preshop" method="post" class="hoverglow">
        <input type="hidden" name="row" value="<%- row %>">
        <input type="hidden" name="index" value="<%- index %>">
        <input type="image" src="/shop.svg" alt="home" class="responsive-svg">
      </form>
    </div>
    <% if (isKidLocation) { %>
      <img src="/kid-head.png" class="kid-head" alt="Kid Head">
    <% } %>
  </div>
  <% break;
  case 'chosen' : %>
  <div class="centered-container <%= isKidLocation ? 'kid-location' : '' %>">
    <div class="svg-wrapper">
      <img src="/shop_red.svg" alt="home" class="responsive-svg">
    </div>
    <% if (isKidLocation) { %>
      <img src="/kid-head.png" class="kid-head" alt="Kid Head">
    <% } %>
  </div>
  <% break;
  case 'notChosen' : %>
  <div class="centered-container <%= isKidLocation ? 'kid-location' : '' %>">
    <div class="svg-wrapper">
      <img src="/shop_red.svg" alt="home" class="responsive-svg">
    </div>
    <% if (isKidLocation) { %>
      <img src="/kid-head.png" class="kid-head" alt="Kid Head">
    <% } %>
  </div>
  <% break;
  case 'inactive' : %>
  <div class="centered-container <%= isKidLocation ? 'kid-location' : '' %>">
    <div class="svg-wrapper">
      <img src="/shop.svg" alt="home" class="responsive-svg">
    </div>
    <% if (isKidLocation) { %>
      <img src="/kid-head.png" class="kid-head" alt="Kid Head">
    <% } %>
  </div>
  <% break;
} %>